{
	"name": "frontend",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start:all": "nx run-many --targets=serve",
		"start:firstApp": "nx serve firstApp",
		"start:secondApp": "nx serve secondApp",
		"build": "nx build",
		"build:prod:firstApp": "nx build firstApp --configuration=production --base-href=https://chavdarovae.github.io/workoutApp/",
		"build:prod:secondApp": "nx build secondApp --configuration=production --base-href=https://chavdarovae.github.io/workoutApp/",
		"build:prod:affected": "nx affected --targets=build --configuration=production --base-href=https://chavdarovae.github.io/workoutApp/",
		"watch": "nx build --watch --configuration development",
		"test": "nx test",
		"deploy": "nx deploy --base-href /workoutApp/",
		"lint:firstApp": "nx lint firstApp",
		"graph": "nx graph"
	},
